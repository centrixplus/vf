<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!--vc_concepts tree view -->
    <record id="res_concept_view_tree" model="ir.ui.view">
        <field name="name">res.concept.view.tree</field>
        <field name="model">res.concept</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- res.concept form view -->
    <record id="res_concept_view_form" model="ir.ui.view">
        <field name="name">res.concept.view.form</field>
        <field name="model">res.concept</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>